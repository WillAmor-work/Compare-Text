USE EMEABB
--6:05
-- Foreign Key
if exists (select * from INFORMATION_SCHEMA.CONSTRAINT_COLUMN_USAGE where TABLE_NAME = 'ACCESSACCOUNTNAMES' and CONSTRAINT_NAME = 'R_1028')
	begin
		PRINT 'Dropping foreign key constraint ACCESSACCOUNTNAMES.R_1028...'
		ALTER TABLE ACCESSACCOUNTNAMES DROP CONSTRAINT R_1028
	end
go

if exists (select * from INFORMATION_SCHEMA.CONSTRAINT_COLUMN_USAGE where TABLE_NAME = 'ACCESSACCOUNTNAMES' and CONSTRAINT_NAME = 'R_1029')
